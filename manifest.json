{
	"name": "Print Nozbe",
	"version": "1.0",
	"manifest_version": 2,
	"description": "A browser extension to easily print Nozbe tasks, especially completed tasks from a specific project.",
	"icons": {"128": "task-printing-exporting-icon.png"},
	"browser_action": {
		"default_icon": "task-printing-exporting-icon.png",
		"default_title": "Print Nozbe Tasks",
		"default_popup": "PageSetup.html"
	},
	"permissions": ["https://webapp.nozbe.com/*", "tabs", "storage"],
	"background": {
		"scripts": ["jquery-2.1.1.js", "background.js"]
	}
}
